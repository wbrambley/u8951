<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources>

    <!-- Set additional audio parameters for incall audio
         Usage: parameter=onstring=offstring
         Examples: <item>realcall=on=off</item>
                   <item>dualmic_enabled=true=false</item>
                   <item>mic_boost=yes=no</item> -->
    <add-resource type="string" name="config_telephony_set_audioparameters">
    <string-array name="config_telephony_set_audioparameters" translatable="true">
    <item>dualmic_enabled=true</item>
    </string-array>
    </add-resource>

</resources>
